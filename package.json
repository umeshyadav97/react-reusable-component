{
  "name": "react-web-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "env-cmd -f .env.dev react-scripts start",
    "start:qa": "env-cmd -f .env.qa react-scripts start",
    "start:stage": "env-cmd -f .env.staging react-scripts start",
    "build": "react-scripts build",
    "build:dev": "GENERATE_SOURCEMAP=false env-cmd -f .env.dev react-scripts build",
    "build:stage": "GENERATE_SOURCEMAP=false env-cmd -f .env.staging react-scripts build",
    "build:qa": "GENERATE_SOURCEMAP=false env-cmd -f .env.qa react-scripts build",
    "build:prod": "GENERATE_SOURCEMAP=false env-cmd -f .env.prod react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prettier:write": "npx prettier --write \"**/*.{js,jsx,json}\" && npx prettier --write \"*.{js,jsx,json}\"",
    "lint": "eslint .",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/lab": "^5.0.0-alpha.116",
    "@mui/material": "^5.11.5",
    "@mui/x-date-pickers": "^5.0.20",
    "@reduxjs/toolkit": "^1.9.1",
    "@stripe/react-stripe-js": "^2.1.0",
    "@stripe/stripe-js": "^1.53.0",
    "axios": "^1.3.0",
    "chart.js": "^3.9.1",
    "date-fns": "^2.30.0",
    "dayjs": "^1.11.7",
    "formik": "^2.2.9",
    "lottie-react": "^2.4.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "react": "^18.2.0",
    "react-chartjs-2": "^4.3.1",
    "react-cookie": "^4.1.1",
    "react-countup": "^6.4.2",
    "react-dates": "^21.8.0",
    "react-day-picker": "^8.6.0",
    "react-dom": "^18.2.0",
    "react-lazy-load-image-component": "^1.5.6",
    "react-pannellum": "^0.2.14",
    "react-phone-number-input": "^3.2.19",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.7.0",
    "react-scripts": "^5.0.1",
    "react-scroll-trigger": "^0.6.14",
    "react-social-login-buttons": "^3.6.1",
    "react-toastify": "^9.1.2",
    "reactjs-social-login": "^2.6.2",
    "redux": "^4.2.0",
    "sass": "^1.57.1",
    "web-vitals": "^3.1.1",
    "yup": "^0.32.11"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@sentry/react": "^7.31.1",
    "@sentry/tracing": "^7.31.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "babel-jest": "^29.3.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.32.0",
    "eslint-plugin-react": "^7.32.1",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "prettier": "2.8.3"
  }
}
